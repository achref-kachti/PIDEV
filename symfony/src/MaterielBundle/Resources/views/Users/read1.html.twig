
<h1>Admin Gestion Users</h1>




{#{{ form(f,{attr: {novalidate: 'novalidate'}}) }}#}

<table border="1">
    <tr>
        <td> id</td>
        <td> nom </td>
        <td> prenom </td>
        <td> email </td>
        <td> motDePasse</td>
        <td> dateNaissance</td>
        <td> telephone</td>

    </tr>
    {% for i in clubs %}
        <tr>
            <td> {{ i.id }} </td>
            <td> {{ i.nom }} </td>
            <td> {{ i.prenom}} </td>
            <td> {{ i.email }} </td>
            <td> {{ i.motDePasse}} </td>


            <td> {{ i.dateNaissance|date('Y-m-d') }}</td>
            <td>{{i.telephone}}</td>

            <td>
                <a href="{{ path('users_remove',
                    { 'id':i.id }) }}">
                    <button> Supprimer </button>
                </a>
            </td>
            <td>
                <a href="{{ path('users_modif',
                    { 'id':i.id }) }}">
                    <button> Modifier </button>
                </a>
            </td>


        </tr>
    {% else %} pas de user
    {% endfor %}

</table>

